<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#0b8db3" />
      <stroke
          android:color="#097ea1" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#0b8db3"
          android:endColor="#097ea1"
          android:angle="270" />
      <stroke
          android:color="#097ea1" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
</selector>
